<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Selenium-Automation" parallel="tests" thread-count="2"
	preserve-order="false" configfailurepolicy="continue" junit="false"
	skipfailedinvocationcounts="false"  data-provider-thread-count="10"
	group-by-instances="false" allow-return-values="false" >
	<listeners>
		<listener class-name="com.web.automation.listeners.ExecutionListener" />
	</listeners>
	<test name="Test-mobile-native" junit="false"
		skipfailedinvocationcounts="false" preserve-order="true"
		group-by-instances="false" allow-return-values="false" >
		<parameter name="platform" value="windows" />
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="6.0.1" />
		<parameter name="environment" value="UAT" />
		<parameter name="userRegion" value="US" />
		<parameter name="seleniumGrid" value="false" />
		<parameter name="localExecution" value="true" />
		<parameter name="cloudExecution" value="false" />
		
		<parameter name="emailReport" value="false" />
		<groups>
			<run>
				<include name="smoke" /> <include name="regression" />
			</run>
		</groups>
		<classes>
				
		   
	       	 
			<class name="com.web.automation.HybrisScripts.Hybris_VerifyBrokenLinks"> </class>       
		   <class name="com.web.automation.HybrisScripts.Hybris_VerifyBrokenLinks_HowRACWorks"> </class> 
		   <class name="com.web.automation.HybrisScripts.Hybris_VerifyBrokenLinks_FAQ"> </class> 
		   <class name="com.web.automation.HybrisScripts.Hybris_VerifyBrokenLinks_RACBenefits"> </class>
		   <class name="com.web.automation.HybrisScripts.Hybris_VerifyBrokenLinks_WhyRent"> </class>
		   <class name="com.web.automation.HybrisScripts.Hybris_VerifyBrokenLinks_ContactUs"> </class>
		   <class name="com.web.automation.HybrisScripts.Hybris_VerifyBrokenLinks_RACCares"> </class>
		   <class name="com.web.automation.HybrisScripts.Hybris_VerifyBrokenLinks_AboutRAC"> </class>
		   <class name="com.web.automation.HybrisScripts.Hybris_VerifyBrokenLinks_OurHistory"> </class>
		   <class name="com.web.automation.HybrisScripts.Hybris_VerifyBrokenLinks_OurBrands"> </class>
			  				
				
				
 	    </classes> 
	</test>
	
</suite> 